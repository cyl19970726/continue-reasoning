{
  "name": "@continue-reasoning/coding",
  "version": "0.1.0",
  "type": "module",
  "description": "Interactive CLI coding assistant for continue-reasoning framework",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "cr": "./dist/start.js"
  },
  "scripts": {
    "build": "tsc --build",
    "dev": "tsc --build --watch",
