{
  "name": "hhh-agi-coding-benchmark",
  "version": "1.0.0",
  "description": "Benchmark suite for testing HHH-AGI coding agent performance",
  "main": "run-agent-test.js",
    "scripts": {
    "test": "node run-agent-test.js --help",
    "test:level1": "node run-agent-test.js level1-basic-operations",
    "test:level2": "node run-agent-test.js level2-code-understanding", 
    "test:level3": "node run-agent-test.js level3-feature-implementation",
    "test:basic": "node run-agent-test.js --basic",
    "test:multiline": "node test-multiline.js",
    "test:cli-task-manager": "node test-cli-task-manager.js",
    "test:optimized": "node run-optimized-test.js",
    "clean": "rm -rf test-workspace reports/*.json",
    "setup": "mkdir -p reports test-workspace"
  },
  "keywords": [
    "ai",
    "coding-agent",
    "benchmark",
    "testing",
    "performance"
  ],
  "author": "HHH-AGI Team",
  "license": "MIT",
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/hhh-agi.git",
    "directory": "tests/coding-agent-benchmark"
  },
  "bugs": {
    "url": "https://github.com/your-org/hhh-agi/issues"
  },
  "homepage": "https://github.com/your-org/hhh-agi#readme"
} 